@page "/crafts/upload-image/{id}"
@model KalaGhar.Pages.Crafts.UploadImageModel
@{
}



<form method="post" enctype="multipart/form-data">

    @for (int i = 0; i < Model.AvailableImage; i++)
    {
        <label asp-for="Images[i]"></label>
        <input type="file" asp-for="Images" multiple />
    }

    <input type="submit" value="Upload" />
</form>